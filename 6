class Animal:
    def voice(self):
        pass

class Dog(Animal):
    def voice(self):
        return "Woof!"

class Cat(Animal):
    def voice(self):
        return "Meow!"

class Cow(Animal):
    def voice(self):
        return "Moo!"

dog = Dog()
cat = Cat()
cow = Cow()

print(dog.voice())
print(cat.voice())
print(cow.voice())
