class Animal:
    count = 0

    def __init__(self):
        Animal.increment_count()

    @classmethod
    def increment_count(cls):
        cls.count += 1

    @classmethod
    def get_count(cls):
        return cls.count

    def voice(self):
        pass  

class Dog(Animal):
    def voice(self):
        return "Woof!"

class Cat(Animal):
    def voice(self):
        return "Meow!"

class Cow(Animal):
    def voice(self):
        return "Moo!"

dog = Dog()
cat = Cat()
cow = Cow()

print(dog.voice())  
print(cat.voice())  
print(cow.voice())  

print(f"Количество созданных экземпляров: {Animal.get_count()}")  
